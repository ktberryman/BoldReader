{
    "manifest_version": 3,
    "name": "BOld REader",
    "version": "1.1",
    "description": "Bionic reader extension.",
    "permissions":[
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"]
        }
    ]
}